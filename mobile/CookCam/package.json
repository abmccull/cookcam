{
  "name": "CookCam",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start-optimized": "react-native start --max-workers $(sysctl -n hw.ncpu)",
    "start-reset": "react-native start --reset-cache",
    "start-fast": "NODE_ENV=development react-native start --max-workers $(sysctl -n hw.ncpu)",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --bundle-type ram",
    "bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --bundle-type ram",
    "test": "jest",
    "metro-profile": "echo 'Press I in Metro console for bundle profile'",
    "cache-clean": "rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "watchman-reset": "watchman watch-del '$PWD' ; watchman watch-project '$PWD'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react-native-push-notification": "^8.1.4",
    "deprecated-react-native-prop-types": "^4.1.0",
    "googleapis": "^149.0.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.4.1",
    "lucide-react-native": "^0.200.0",
    "memoize-one": "^6.0.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-native": "0.73.9",
    "react-native-config": "^1.5.1",
    "react-native-deck-swiper": "^2.0.17",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-iap": "^12.16.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.6.3",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "~13.14.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vision-camera": "^3.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-minify-terser": "^0.82.4",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "@react-native-community/cli": "12.3.7",
    "@react-native-community/cli-config": "12.3.7",
    "@react-native-community/cli-debugger-ui": "12.3.7",
    "@react-native-community/cli-doctor": "12.3.7",
    "@react-native-community/cli-platform-android": "12.3.7",
    "@react-native-community/cli-platform-ios": "12.3.7",
    "@react-native-community/cli-server-api": "12.3.7",
    "@react-native-community/cli-tools": "12.3.7",
    "@react-native-community/cli-types": "12.3.7"
  }
}
