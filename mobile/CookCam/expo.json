{
  "expo": {
    "name": "CookCam",
    "slug": "cookcam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cookcam.app",
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "CookCam needs camera access to scan ingredients for recipe generation.",
        "NSPhotoLibraryUsageDescription": "CookCam needs photo library access to save and share recipe images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.abmccull.cookcamexpo",
      "jsEngine": "hermes",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./plugins/withIAPStore.js",
      "expo-camera",
      "expo-secure-store",
      "react-native-reanimated/plugin",
      "expo-notifications",
      "expo-dev-client"
    ],
    "extra": {
