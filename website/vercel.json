{
  "rewrites": [
    {
      "source": "/ref/:code",
      "destination": "/ref.html"
    },
    {
      "source": "/c/:slug", 
      "destination": "/c.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "functions": {
    "ref.html": {
      "includeFiles": "ref.html"
    },
    "c.html": {
      "includeFiles": "c.html"
    }
  }
} 