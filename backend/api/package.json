{
  "name": "cookcam-api",
  "version": "1.0.0",
  "description": "CookCam Backend API - Production Build",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src --ext .ts --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts'",
    "lint:fix": "eslint src --ext .ts --fix --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts'",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "seed-usda:complete": "node dist/scripts/complete-usda-seeder.js run",
    "seed-usda:complete:resume": "node dist/scripts/complete-usda-seeder.js resume",
    "seed-usda:complete:status": "node dist/scripts/complete-usda-seeder.js status",
    "monitor-usda": "node dist/scripts/monitor-usda-seeding.js monitor",
    "monitor-usda:quick": "node dist/scripts/monitor-usda-seeding.js quick",
    "seed-usda:legacy": "node dist/scripts/usda-bulk-seeder.js run",
    "generate:openapi": "ts-node src/scripts/generate-openapi.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "@types/ioredis": "^4.28.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^5.0.1",
    "pg": "^8.16.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "jest": "^30.0.5",
    "msw": "^2.10.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "cookcam",
    "api",
    "recipes"
  ],
  "author": "CookCam Team",
  "license": "ISC"
}
