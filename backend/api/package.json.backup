{
  "name": "cookcam-api",
  "version": "1.0.0",
  "description": "CookCam Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --format=stylish",
    "lint:fix": "eslint src --fix --format=stylish",
    "lint:check": "eslint src --format=stylish --max-warnings 0",
    "lint:errors": "eslint src --format=stylish --quiet",
    "lint:strict": "eslint src --max-warnings 0",
    "lint:routes": "eslint src/routes --max-warnings 70",
    "pre-commit": "npm run lint:routes && npm run build",
    "production:check": "npm run build && npm run lint:errors",
    "seed-usda": "ts-node src/scripts/usda-bulk-seeder.ts",
    "seed-usda:run": "ts-node src/scripts/usda-bulk-seeder.ts run",
    "seed-usda:resume": "ts-node src/scripts/usda-bulk-seeder.ts resume",
    "seed-usda:status": "ts-node src/scripts/usda-bulk-seeder.ts status",
    "seed-usda:test": "ts-node src/scripts/test-seeding.ts",
    "db:status": "ts-node src/scripts/quick-db-status.ts",
    "env:check": "ts-node src/scripts/environment-check.ts",
    "usda:test": "ts-node src/scripts/test-usda-connection.ts",
    "monitor": "ts-node src/scripts/seeding-monitor.ts",
    "monitor:status": "ts-node src/scripts/seeding-monitor.ts status",
    "monitor:progress": "ts-node src/scripts/seeding-monitor.ts progress",
    "migrate:gamification": "ts-node src/scripts/create-gamification-tables.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "@types/pg": "^8.15.2",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "aws-sdk": "^2.1518.0",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^149.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.0.1",
    "pg": "^8.16.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "cookcam",
    "api",
    "recipes",
    "gamification"
  ],
  "author": "CookCam Team",
  "license": "ISC"
}
